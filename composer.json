      "tests/helper/str_rand.php",
      "tests/helper/temp_dir.php"
    ]
  },
  "authors": [
    {
      "name": "takuya",
      "email": "55338+takuya@users.noreply.github.com"
    }
  ],
  "require": {
    "php": ">=8.1",
    "takuya/php-sysv-ipc-semaphore": "^0.3",
    "ext-shmop": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^10",
    "ext-pcntl": "*"
  }
}
