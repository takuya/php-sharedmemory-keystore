{
  "name": "takuya/php-sharedmemory-keystore",
  "description": "php shmop_xx wrapper",
  "type": "library",
  "license": "GPL-3.0-or-later",
  "keywords": [
    "php",
    "sysV",
    "IPC",
    "SharedMemory",
    "shmop"
  ],
  "autoload": {
    "psr-4": {
